{
    "id": "io.gitlab.adhami3310.Impression",
    "runtime": "org.gnome.Platform",
    "runtime-version": "45",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "impression",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--system-talk-name=org.freedesktop.UDisks2",
        "--share=network"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/impression/cargo"
        }
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "blueprint-compiler",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "git",
                    "url": "https://gitlab.gnome.org/jwestman/blueprint-compiler.git",
                    "tag": "v0.8.1",
                    "commit": "aa7679618e864748f4f4d8f15283906e712752fe"
                }
            ]
        },
        {
            "name": "impression",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.com/adhami3310/Impression/uploads/be70c187016ee23d7ba0f5a0a2d89c41/impression-3.1.0.tar.xz",
                    "sha256": "d1a1363fc088d4adfdca1536923cb7379155d74adcfac616075315656f3a5c2d"
                }
            ]
        }
    ]
}
